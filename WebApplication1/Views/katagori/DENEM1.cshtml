@using entity.conti
@model List<katagori>

@{
    ViewData["Title"] = "DENEM1";
    Layout = "~/Views/Shared/writerpanel.cshtml";
}

<table class="table table-bordered">



    <tr>
        <th>ID</th>
        <th>KATAGROİ ADI</th>
        <th>DURUM</th>
        <th>DETAYLAR</th>
        <th>SİL</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td><a>@item.katagoriID</a> </td>
            <td>@item.katagoriName </td>
            <td>@item.katagoristatus </td>
            <td><a href="#" class="btn btn-warning"> detaylar</a>  </td>
            <td><a href="#" id="DeleteBtn" onclick="return confirm('silmek istediğine eminmisin')" class="btn btn-danger"> sil</a>  </td>
        </tr>

    }

</table>
