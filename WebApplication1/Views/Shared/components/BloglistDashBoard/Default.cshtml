@using entity.conti
@model List<blog>
<div class="row">
    <div class="col-12 grid-margin">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Recent Tickets</h4>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th> Assignee </th>
                                <th> Subject </th>
                                <th> Status </th>
                                <th> Last Update </th>
                                <th> Tracking ID </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                
                         
                            <tr>
                                <td>
                                        <img src="@item.blogküçükresim" class="mr-2" alt="image">  @item.title
                                </td>
                                <td> qqqqqqqqqqqqqqqqqqqqqqqqq </td>
                                <td>
                                    <label class="badge badge-gradient-success"> @item.katagorı.katagoriName</label>
                                </td>
                                    <td> @(((DateTime)item.creatdate).ToString("dd-MMM-yyyy")) </td>
                                <td> WD-12345 </td>
                            </tr>
                           }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>