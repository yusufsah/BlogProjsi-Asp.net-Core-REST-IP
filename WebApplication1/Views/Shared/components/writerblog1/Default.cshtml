@using entity.conti
@model List<blog>





<div class="tech-btm">
	<h4>yazarların diğer yazıları </h4>


	@foreach (var item in Model)
	{
		
	

	<div class="blog-grids row mb-3">
		<div class="col-md-5 blog-grid-left">
			<a href="single.html">
				<img src="@item.blogküçükresim" class="img-fluid" alt="">
			</a>
		</div>
		<div class="col-md-7 blog-grid-right">

			<h5>
				<a href="single.html">@item.title </a>
			</h5>
			<div class="sub-meta">
				<span>
					<i class="far fa-clock"></i> @(((DateTime)item.creatdate).ToString("dd-MMM-yyyy"))
				</span>
			</div>
		</div>

	</div>

	}

</div>